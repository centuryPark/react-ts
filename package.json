{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "webpack react",
  "private": true,
  "main": "build/webpack.config.js",
  "scripts": {
    "build": "rm -rf dist/&&webpack --config build/webpack.prod.js",
    "build:analysis": "export analysis=true&&npm run build",
    "start": "webpack-dev-server --open --hot --config build/webpack.dev.js",
    "serve": "node server/server.js",
    "lint:js": "eslint --ext .js server",
    "lint:ts": "tslint 'src/**/*.{ts,tsx}'",
    "lint:style": "stylelint stylelint --syntax scss **/*.scss",
    "lint": "npm run lint:js&&npm run lint:style&&npm run lint:ts",
    "precommit": "lint-staged"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "park",
  "license": "ISC",
  "browserslist": [
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4",
    "ie >= 9"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:ts",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run lint:style",
      "git add"
    ],
    "server/**/*.js": [
      "npm run lint:js",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@types/react-redux": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/history": "^4.7.2",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-loadable": "^5.5.0",
    "@types/react-router-dom": "^4.3.1",
    "axios": "0.19.0",
    "connect-history-api-fallback": "^1.5.0",
    "connected-react-router": "^5.0.1",
    "date-fns": "^1.30.1",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.19.0",
    "koa": "^2.5.3",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
